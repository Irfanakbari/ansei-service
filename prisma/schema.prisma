// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model RawData {
  id              Int     @id @default(autoincrement())
  po_id           String
  date            String?
  delivery_date   String
  part_no         String
  part_name       String
  quantity        Int
  id_part         String
  barcode         String
  receiving_area  String
  po_no           String
  model           String
  bagian_part     String
  delivery_period String

  @@map("raw_data") // Optional: Jika nama tabel di database tidak sama dengan nama model
}

model History {
  id        Int      @id @default(autoincrement())
  timestamp DateTime @default(now()) @db.Timestamp()
  po_no     String
  po_id     String
  status    String
  part_no   String
  operator  String

  @@map("history") // Optional: Jika nama tabel di database tidak sama dengan nama model
}

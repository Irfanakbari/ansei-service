// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model RawData {
  id             Int     @id @default(autoincrement())
  date           String
  vendorCode     String
  vendorName     String
  receivingArea  String
  deliveryDate   String
  deliveryPeriod Int
  firm           String?
  classification String?
  poNumber       String?
  item           Int
  partsNumber    String
  partsName      String
  orderQuantity  Int

  @@map("raw_data") // Optional: Jika nama tabel di database tidak sama dengan nama model
}

model history {
  id           Int      @id @default(autoincrement())
  // id_part     String   @db.VarChar(100)
  timestamp    DateTime @default(now()) @db.Timestamp()
  po_number    String   @db.VarChar(255)
  parts_number String   @db.VarChar(255)
  status       String?  @db.VarChar(100)
  operator     String?  @db.VarChar(100)
}
